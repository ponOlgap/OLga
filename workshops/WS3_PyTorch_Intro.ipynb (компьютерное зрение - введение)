        "epochs = 100\n",
        "losses = np.zeros(epochs)\n",
        "\n",
        "for i in range(epochs):\n",
        "    predict = model.forward(x)\n",
        "    \n",
        "    loss = criterion(predict, y)\n",
        "\n",
        "    losses[i] = loss.data.cpu().numpy()\n",
        "    \n",
        "    optimizer.zero_grad()\n",
        "    \n",
        "    loss.backward()\n",
        "    \n",
        "    optimizer.step()\n",
        "    \n",
        "    if(i//20 == i/20):\n",
        "        print(\"Epoch: \", i+1, \" Loss: \", loss.item())\n",
        "\n",
        "plt.plot(losses)"
      ]
