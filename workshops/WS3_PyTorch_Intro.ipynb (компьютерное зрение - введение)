        "Однако, некоторые операции в торч называются под другому."
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "siai1xdqd_mZ",
        "outputId": "cc65b583-2ea9-4201-972f-5f26b02f78b3"
      },
      "outputs": [
        {
          "name": "stdout",
          "output_type": "stream",
          "text": [
            "tensor([[237.1052, 242.7137, 298.6630],\n",
            "        [242.7137, 252.1152, 309.6765],\n",
            "        [298.6630, 309.6765, 380.4629]])\n",
            "tensor([[0.4000, 0.4000, 0.4000],\n",
            "        [0.4000, 0.4000, 0.4000],\n",
            "        [0.4000, 0.4000, 0.4000]])\n",
            "tensor([[0.4000, 0.4000, 0.4000, 0.4000, 0.4000, 0.4000, 0.4000, 0.4000, 0.4000]])\n",
            "tensor([[[0.4000, 0.4000, 0.4000, 0.4000, 0.4000, 0.4000, 0.4000, 0.4000,\n",
            "          0.4000]]])\n",
            "tensor([0.4000, 0.4000, 0.4000])\n",
            "tensor(0.4000)\n"
          ]
        }
      ],
      "source": [
        "z = s.mm(s.T) #matrix multiplication\n",
        "print(z)\n",
        "\n",
        "z = torch.mm(y.T, m) #matrix multiplication\n",
        "print(z)\n",
        "\n",
        "print(z.view(1,- 1)) #reshape\n",
        "\n",
        "print(z.view(1,1,-1))# Добавим размерность\n",
        "\n",
        "z = torch.mv(m.T,y[:,0]) #matrix to vector multiplication\n",
        "print(z)\n",
        "\n",
        "z = torch.dot(m[:,0],y[:,1]) #inner vector product\n",
        "print(z)"
      ]
    },
